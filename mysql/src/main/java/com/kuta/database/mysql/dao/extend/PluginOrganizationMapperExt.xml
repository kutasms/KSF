	<resultMap type="com.kuta.database.mysql.pojo.extend.PluginOrganizationExt"  
	id="ResultMapExt"  
	extends="com.kuta.database.mysql.dao.PluginOrganizationMapper.BaseResultMap">
		<collection property="scripts" column="pid" ofType="com.kuta.database.mysql.pojo.extend.PluginScriptWithRel"  select="com.kuta.database.mysql.dao.extend.PluginScriptRelMapperExt.getScripts"></collection>
		<collection property="params"  column="poid"  ofType="com.kuta.database.mysql.pojo.PluginOrganizationParam" select="com.kuta.database.mysql.dao.extend.PluginOrganizationParamMapperExt.getList"></collection>
		<collection property="paramTemplates" column="pid" ofType="com.kuta.database.mysql.pojo.PluginParamTemplate" select="com.kuta.database.mysql.dao.extend.PluginParamTemplateMapperExt.getList"></collection>
		
	</resultMap>
	
	<select id="selectExtByPrimaryKey" parameterType="java.lang.Integer" resultMap="ResultMapExt">
    select 
    <include refid="com.kuta.database.mysql.dao.PluginOrganizationMapper.Base_Column_List" />
    from BS_Plugin_Org
    where `poid` = #{poid,jdbcType=INTEGER}
  </select>
