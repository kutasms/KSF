<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kuta.database.mysql.dao.extend.PluginHttpRequestMapperExt">
  <resultMap type="com.kuta.database.mysql.pojo.extend.PluginHttpRequestExt"  
	id="ResultMapWithHeaders"  
	extends="com.kuta.database.mysql.dao.PluginHttpRequestMapper.BaseResultMap">
		<collection property="headers"  column="{phrid=phrid,type=type}"  ofType="com.kuta.database.mysql.pojo.PluginHttpHeader" select="com.kuta.database.mysql.dao.extend.PluginHttpHeaderMapperExt.getHeaders"></collection>
	</resultMap>
  
  <select id="selectExtByPrimaryKey"  resultMap="ResultMapWithHeaders">
    select 
    <include refid="com.kuta.database.mysql.dao.PluginHttpRequestMapper.Base_Column_List" />
    ,1 as `type`
    from BS_Plugin_Http_Report
    where `phrid` = #{phrid,jdbcType=INTEGER}
  </select>
  <select id="selectExtByExample" parameterType="com.kuta.database.mysql.pojo.PluginHttpRequestExample" resultMap="ResultMapWithHeaders">
select
    <if test="distinct">
      distinct
    </if>
    'true' as QUERYID,
    <include refid="com.kuta.database.mysql.dao.PluginHttpRequestMapper.Base_Column_List" />
     ,1 as `type`
    from BS_Plugin_Http_Request
    <if test="_parameter != null">
      <include refid="com.kuta.database.mysql.dao.PluginHttpRequestMapper.Example_Where_Clause" />
    </if>
    <if test="orderByClause != null">
      order by ${orderByClause}
    </if>
  </select>
</mapper>